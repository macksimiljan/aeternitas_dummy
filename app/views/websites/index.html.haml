%h1
  Websites
%ul
  - @websites.each do |website|
    %li
      = "#{website.id}, #{website.url}:"
      %ul
        %li= "Merkel: #{website.merkel_word_count}"
        %li= "Schulz: #{website.schulz_word_count}"

%p= link_to 'Create a new website', new_website_path
%p= link_to 'Dashboard', 'dashboard#index'
